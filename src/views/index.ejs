<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/header') %> 
    <title>Ejemplo de Prueba</title>
</head>
<body>
    <div class="container">
        <% books.forEach(function(book){ %>
            <div class="card">
            <div class="card-title">
                <%= book.title %>
            </div>
            <div class="card-body">
                <%= book.author %>
            </div>
        </div>
      <% }); %>    
    </div>

 <%- include('partials/footer') %>    
</body>
</html>